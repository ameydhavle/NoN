<options xmlns="http://marklogic.com/appservices/search">
  <search-option>unfiltered</search-option>
  <page-length>10</page-length>
  <!--
<additional-query>
<cts:and-query xmlns:cts="http://marklogic.com/cts">
  <cts:collection-query>
  <cts:uri>call-center</cts:uri>
    <cts:uri>customers</cts:uri>
    <cts:uri>documents</cts:uri>
  </cts:collection-query>

  <cts:not-query xmlns:cts="http://marklogic.com/cts">
    <cts:collection-query>
      <cts:uri>binary</cts:uri>
      <cts:uri>processed-binary</cts:uri>
      <cts:uri>linkedData</cts:uri>
      <cts:uri>http://marklogic.com/semantics#default-graph</cts:uri>
      <cts:uri>http://marklogic.com/semantics#graphs</cts:uri>

     <cts:uri>call-center</cts:uri>
      <cts:uri>zipcodes</cts:uri>
    </cts:collection-query>
  </cts:not-query>
      <cts:not-query xmlns:cts="http://marklogic.com/cts">
        <cts:directory-query>
          <cts:uri>/users/</cts:uri>
        </cts:directory-query>
      </cts:not-query>
    </cts:and-query>
  </additional-query>
  -->
  <term apply="term">
    <empty apply="all-results" />
    <term-option>punctuation-insensitive</term-option>
    <term-option>stemmed</term-option>
  </term>
  <constraint name="Collection">
    <collection prefix=""/>
  </constraint>

  <constraint name="Last-Updated-By">
    <range type="xs:string" facet="true" collation="http://marklogic.com/collation/en/S1">
      <facet-option>limit=10</facet-option>
      <facet-option>frequency-order</facet-option>
      <facet-option>descending</facet-option>
      <element ns="" name="last_updated_by"/>
    </range>
  </constraint>
  <default-suggestion-source>
    <word-lexicon collation="http://marklogic.com/collation/"></word-lexicon>
  </default-suggestion-source>
  <grammar>
    <quotation>"</quotation>
    <implicit>
      <cts:and-query strength="20" xmlns:cts="http://marklogic.com/cts" />
    </implicit>
    <starter strength="30" apply="grouping" delimiter=")">(</starter>
    <starter strength="40" apply="prefix" element="cts:not-query">-</starter>
    <joiner strength="10" apply="infix" element="cts:or-query" tokenize="word">OR</joiner>
    <joiner strength="20" apply="infix" element="cts:and-query" tokenize="word">AND</joiner>
    <joiner strength="30" apply="infix" element="cts:near-query" tokenize="word">NEAR</joiner>
    <joiner strength="30" apply="near2" consume="2" element="cts:near-query">NEAR/</joiner>
    <joiner strength="50" apply="constraint">:</joiner>
    <joiner strength="50" apply="constraint" compare="LT" tokenize="word">LT</joiner>
    <joiner strength="50" apply="constraint" compare="LE" tokenize="word">LE</joiner>
    <joiner strength="50" apply="constraint" compare="GT" tokenize="word">GT</joiner>
    <joiner strength="50" apply="constraint" compare="GE" tokenize="word">GE</joiner>
    <joiner strength="50" apply="constraint" compare="NE" tokenize="word">NE</joiner>
  </grammar>
  <sort-order direction="descending">
    <score />
  </sort-order>
  <transform-results apply="snippet">
    <preferred-elements>
      <element ns="" name="body" />
    </preferred-elements>
    <max-matches>1</max-matches>
    <max-snippet-chars>150</max-snippet-chars>
    <per-match-tokens>20</per-match-tokens>
  </transform-results>
  <return-query>1</return-query>
  <operator name="results">
    <state name="compact">
      <transform-results apply="snippet">
        <preferred-elements>
          <element ns="" name="body" />
        </preferred-elements>
        <max-matches>1</max-matches>
        <max-snippet-chars>150</max-snippet-chars>
        <per-match-tokens>20</per-match-tokens>
      </transform-results>
    </state>
    <state name="detailed">
      <transform-results apply="snippet">
        <preferred-elements>
          <element ns="" name="body" />
        </preferred-elements>
        <max-matches>2</max-matches>
        <max-snippet-chars>400</max-snippet-chars>
        <per-match-tokens>30</per-match-tokens>
      </transform-results>
    </state>
  </operator>

  <constraint name="geo-point">
    <custom facet="true">
      <parse apply="parse-structured" ns="http://marklogic.com/appservices/viz/geo" at="/ext/constraint/geo_ml8.xqy" />
      <start-facet apply="start" ns="http://marklogic.com/appservices/viz/geo" at="/ext/constraint/geo_ml8.xqy" />
      <finish-facet apply="finish" ns="http://marklogic.com/appservices/viz/geo" at="/ext/constraint/geo_ml8.xqy" />
    </custom>
    <annotation>
      <geo-elem-pair>
        <heatmap n="90" e="180" s="-90" w="-180" latdivs="6" londivs="6"/>
        <parent ns="" name="geo-data"/>
        <lat ns="" name="Lat"/>
        <lon ns="" name="Long"/>
      </geo-elem-pair>
    </annotation>
  </constraint>

  <extract-metadata>

    <qname elem-ns="" elem-name="Company_Name"/>
    <qname elem-ns="" elem-name="Industry"/>
    <qname elem-ns="" elem-name="SIC_Code_Description"/>
    <qname elem-ns="" elem-name="SIC_Code"/>
    <qname elem-ns="" elem-name="Address"/>
    <qname elem-ns="" elem-name="City"/>
    <qname elem-ns="" elem-name="State"/>
    <qname elem-ns="" elem-name="Zip"/>
    <qname elem-ns="" elem-name="Lat"/>
    <qname elem-ns="" elem-name="Long"/>

		<qname elem-ns="" elem-name="TRN_DATE"/>
		<qname elem-ns="" elem-name="DIRECTION"/>
		<qname elem-ns="" elem-name="CDT_NAME1"/>
		<qname elem-ns="" elem-name="DBT_NAME1"/>
		<qname elem-ns="" elem-name="DBT_AMT"/>
		<qname elem-ns="" elem-name="NO_TRANS"/>
		<qname elem-ns="" elem-name="CDT_AMT"/>



  </extract-metadata>
</options>
