  <term apply="term">
    <empty apply="all-results" />
    <term-option>punctuation-insensitive</term-option>
    <term-option>stemmed</term-option>
  </term>
  <sort-order direction="ascending" type="xs:string">
    <element name="last_name"/>
  </sort-order>
  <constraint name="Gender">
    <range type="xs:string" facet="true" collation="http://marklogic.com/collation/en/S1">
      <facet-option>limit=10</facet-option>
      <facet-option>frequency-order</facet-option>
      <facet-option>descending</facet-option>
      <element ns="" name="normalizedGender"/>
    </range>
  </constraint>
  <constraint name="company_name">
    <range type="xs:string" facet="true" collation="http://marklogic.com/collation/en/S1">
      <facet-option>limit=10</facet-option>
      <facet-option>frequency-order</facet-option>
      <facet-option>descending</facet-option>
      <element ns="" name="company_name"/>
    </range>
  </constraint>
  <constraint name="company_type">
    <range type="xs:string" facet="true" collation="http://marklogic.com/collation/en/S1">
      <facet-option>limit=10</facet-option>
