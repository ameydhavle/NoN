      <facet-option>descending</facet-option>
      <element ns="" name="company_type"/>
    </range>
  </constraint>
  <constraint name="company_category">
    <range type="xs:string" facet="true" collation="http://marklogic.com/collation/en/S1">
      <facet-option>limit=10</facet-option>
      <facet-option>frequency-order</facet-option>
      <facet-option>descending</facet-option>
      <element ns="" name="company_category"/>
    </range>
  </constraint>
  <constraint name="Spending_Category">
    <range type="xs:string" facet="true" collation="http://marklogic.com/collation/en/S1">
      <facet-option>limit=10</facet-option>
      <facet-option>frequency-order</facet-option>
      <facet-option>descending</facet-option>
      <element ns="" name="Spending_Category"/>
    </range>
  </constraint>

  <constraint name="Tags">
    <range type="xs:string" facet="true" collation="http://marklogic.com/collation/en/S1">
      <facet-option>limit=10</facet-option>
      <facet-option>frequency-order</facet-option>
      <facet-option>descending</facet-option>
      <element ns="" name="tags"/>
    </range>
  </constraint>
  <constraint name="Service-Area">
    <range type="xs:string" facet="true" collation="http://marklogic.com/collation/en/S1">
      <facet-option>limit=10</facet-option>
      <facet-option>frequency-order</facet-option>
      <facet-option>descending</facet-option>
      <element ns="" name="Service_Area"/>
    </range>
  </constraint>

  <constraint name="Transaction-Size">
    <range type="xs:int" facet="true" collation="">
      <bucket name="Under 1000" ge="0" lt="1000"/>
      <bucket name="1000-5000" ge="1001" lt="5000"/>
      <bucket name="5001-10000" ge="5001" lt="10000"/>
      <bucket name="10001-50000" ge="10001" lt="50000"/>
      <bucket name="50001-100000" ge="50001" lt="1000000"/>
      <element ns="" name="Amount"/>
    </range>
  </constraint>
  <constraint name="Age-Group">
    <range type="xs:int" facet="true" collation="">
      <bucket name="Under 20" ge="13" lt="20"/>
      <bucket name="20-30" ge="20" lt="30"/>
      <bucket name="30-40" ge="30" lt="40"/>
      <bucket name="40-55" ge="40" lt="55"/>
      <bucket name="55+" ge="55" lt="100"/>
      <element ns="" name="Age"/>
    </range>
  </constraint>

  <constraint name="City">
    <range type="xs:string" facet="true" collation="http://marklogic.com/collation/en/S1">
      <facet-option>limit=10</facet-option>
      <facet-option>frequency-order</facet-option>
      <facet-option>descending</facet-option>
